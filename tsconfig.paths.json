{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/@core/components/*"],
      "@src/*": ["src/*"],
      "@public/*": ["public/*"],
      "@assets/*": ["src/@core/assets/*"],
      "@layouts/*": ["src/@core/layouts/*"],
      "@store/*": ["src/redux/*"],
      "@styles/*": ["src/@core/scss/*"],
      "@assets-custom/*": ["src/assets/*"],
      "@configs/*": ["src/configs/*"],
      "@utils/*": ["src/utility/*"],
      "@hooks/*": ["src/utility/hooks/*"],
      "@layoutStore/*": ["src/layouts/*"]
    }
  }
}